{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Anchor Browser Docs",
  "colors": {
    "primary": "#faa380",
    "light": "#fa804f",
    "dark": "#faa380"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Get Started",
            "pages": [
              "introduction",
              {
                "group": "Quickstart",
                "pages": [
                  "quickstart/use-via-code",
                  "quickstart/use-via-api",
                  "quickstart/embed-browser-in-your-product"
                ]
              },
              {
                "group": "Example Use-Cases",
                "pages": [
                  "examples/form-filling",
                  "examples/buyer-intent",
                  "examples/configuration-collection",
                  "examples/research-task"
                ]
              }
            ]
          },
          {
            "group": "Capabilities",
            "pages": [
              "essentials/recording",
              "advanced/session-termination",
              "advanced/proxy",
              "advanced/captcha-solving",
              "advanced/event-coordination",
              {
                "group": "Agentic",
                "icon": "robot",
                "pages": [
                  "agentic/ai-task-completion",
                  "agentic/agentic-file-usage"
                ]
              },
              "advanced/os-level-control",
              "essentials/authentication-and-identity",
              "essentials/tool-building",
              {
                "group": "Files Upload / Download",
                "icon": "file",
                "pages": [
                  "advanced/file-upload",
                  "advanced/file-download",
                  "advanced/p2p-downloads"
                ]
              },
              "advanced/extensions"
              
            ]
          },
          {
            "group": "Integrations",
            "pages": [
              "advanced/browser-live-view",
              "integrations/browseruse-deployment",
              "integrations/openai-cua",
              "agent-frameworks/crewai",
              "agent-frameworks/langchain",
              "agent-frameworks/custom-agent-framework"
            ]
          },
          {
            "group": "Additional Details",
            "pages": [
              "security",
              "pricing"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "versions": [
          {
            "version": "Latest - v1",
            "default": true,
            "openapi": {
              "source": "/openapi.yaml",
              "directory": "api-reference"
            }
          },
          {
            "version": "Legacy",
            "openapi": {
              "source": "/legacy_openapi.yaml",
              "directory": "api-reference/legacy"
            }
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Interactive Playground",
          "href": "https://app.anchorbrowser.io/",
          "icon": "window-restore"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@anchorbrowser.io"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Live Playground",
      "href": "https://app.anchorbrowser.io/playground"
    }
  },
  "footer": {
    "socials": {
      "x": "https://x.com/anchorbrowser"
    }
  }
}